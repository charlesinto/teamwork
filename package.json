  },
  "homepage": "https://github.com/charlesinto/teamwork#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.19.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.9",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "istanbul": "^0.4.5",
    "jasmine": "^3.5.0",
    "jasmine-console-reporter": "^3.1.0",
    "jasmine-node": "^3.0.0",
    "jasmine-supertest": "^1.0.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^5.2.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.3.1",
    "nodemon": "^2.0.1",
    "nyc": "^14.1.1",
    "pg": "^7.14.0",
    "socket.io": "^2.3.0",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "eslint": "^6.7.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2"
  }
